plugin.tx_cynewsletter {
	view {
		templateRootPaths {
			0 = EXT:cy_newsletter/Resources/Private/Templates/
			1 = {$plugin.tx_cynewsletter.view.templateRootPath}
		}

		partialRootPaths {
			0 = EXT:cy_newsletter/Resources/Private/Partials/
			1 = {$plugin.tx_cynewsletter.view.partialRootPath}
		}

		layoutRootPaths {
			0 = EXT:cy_newsletter/Resources/Private/Layouts/
			1 = {$plugin.tx_cynewsletter.view.layoutRootPath}
		}
	}
}

